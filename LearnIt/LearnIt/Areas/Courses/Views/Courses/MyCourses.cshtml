@model IEnumerable<MyCourseInfo>
<h2>My Courses</h2>
<div class="container">
    <div class="row">
        @if (Model == null)
        {
            <h3>Nobody likes you</h3>
        }
        else
        {
            foreach (var item in Model)
            {
                Html.RenderPartial("_MyCourseThumbnail", item);
            }
        }

    </div>
</div>


